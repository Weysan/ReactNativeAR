{
  "expo": {
    "name": "ARProject",
    "slug": "ARProject",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios"
    ],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.raphaelgoncalves.arproject",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use location to...",
        "NSCameraUsageDescription": "We use camera to enable the AR capabilities.",
        "ITSAppUsesNonExemptEncryption": false
      }
    }
  }
}
